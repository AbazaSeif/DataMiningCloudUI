extends base

block scripts
  script(src='/scripts/repo.js')

block breadcrumb
  li Models

block content
  .col-md-7
    table(
      class="table table-hover"
      id='modelsTable'
    )
      each val in models
        tr
          td
            a(href= "/ui/models/" + val._id)
              text= val.metadata.name
